{
    "version": 2,
    "builds": [
        {
            "src": "./index.ts",
            "use": "@vercel/node"
        }
    ],
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "/"
        }
    ],
    "functions": {
        "index.ts": {
            "maxDuration": 120
        }
    }
}